{
    "name": "fractas/google-dfp",
    "description": "Fast, configurable SilverStripe implementation of Google DoubleClick for Publishers (DFP)",
    "type": "silverstripe-vendormodule",
    "homepage": "https://github.com/fractaslabs/silverstripe-google-dfp/",
    "keywords": ["silverstripe", "DoubleClick", "DoubleClick for Publishers", "ads"],
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/fractaslabs/silverstripe-google-dfp/issues"
    },
    "authors": [{
        "name": "Milan Jelicanin",
        "email": "milan.jelicanin@fractas.com",
        "homepage": "https://www.fractas.com/"
    }, {
        "name": "Petar Simic",
        "email": "petar.simic@fractas.com",
        "homepage": "https://www.fractas.com/"
    }],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "silverstripe/vendor-plugin": "^1.0",
        "silverstripe/framework": "^4.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Fractas\\GoogleDfp\\": "src/"
        }
    }
}
